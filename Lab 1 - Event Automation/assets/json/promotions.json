{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Promo Schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "valid_from": {
      "type": "string",
      "format": "date"
    },
    "valid_to": {
      "type": "string",
      "format": "date"
    },
    "category": {
      "type": "string"
    }
  },
  "required": ["id", "title", "description", "valid_from", "valid_to", "category"]
}
